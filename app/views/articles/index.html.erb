<table>
    <tr>
        <th>Id</th>
        <th>Title</th>
        <th>Description</th>
        <th colspan=3>Actions</th>
    </tr>
    <% @articles.each do |each_article| %>
    <tr>
        <td><%= each_article.id%></td>
        <td><%=each_article.title%></td>
        <td><%= each_article.desc%></td>
        <td><%= link_to 'show', article_path(each_article) %></td>
        <td><%= link_to 'update', edit_article_path(each_article) %></td>
        <td><%= link_to 'delete', article_path(each_article), method: :delete, data: {confirm: "Are you sure ?"}  %></td>
    </tr>
    <% end %>
</table>
<br>
<%= link_to 'create new article', new_article_path %>